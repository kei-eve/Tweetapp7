<h1>アカウント編集</h1>
<div>
    <% @user.errors.full_messages.each do |message| %>
    <%= message %>
    <% end %>
</div>
<div>
    <%= form_tag("/users/#{@user.id}/update") do %>
    <div>
        <p>ユーザー名</p>
        <input name="name" value=<%= @user.name %>>
        <p>メールアドレス</p>
        <input name="email" value=<%= @user.email %>>
        <input type="submit" value="更新">
    </div>
    <% end %>
</div>